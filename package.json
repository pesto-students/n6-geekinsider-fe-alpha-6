  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint --ext js,jsx,ts,tsx --max-warnings 0 -f codeframe --cache --color src",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.+(json|yml|yaml|css|scss|ts|tsx|md|mdx)\"",
    "validate": "npm run lint && npm run prettier -- --list-different && tsc",
    "precommit": "lint-staged && tsc",
    "typecheck": "tsc",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged npm run test -- --watchAll=false"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint:fix",
      "npm run test:staged",
      "npm run format"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://geekinsider.click/",
